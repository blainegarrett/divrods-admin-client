    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "concurrently": "^2.2.0",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^4.0.5",
    "eslint": "^3.3.1",
