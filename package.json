{
  "name": "mplsart-web",
  "version": "0.0.1",
  "description": "MPLSART Browser client",
  "scripts": {
    "start": "cross-env NODE_ENV=production PORT=8080 NODE_PATH='./src' node ./build/server/index.js",
    "dev": "cross-env NODE_ENV=development PORT=3000 NODE_PATH='./src' node ./src/server/index.js",
    "build": "npm run build-client; npm run build-server",
    "build-client": "cross-env NODE_ENV=production webpack --config webpack/prod.client.config.js --verbose --colors --display-error-details ",
    "build-server": "cross-env NODE_ENV=production webpack --verbose --colors --display-error-details --config webpack/prod.server.config.js",
    "deploy": "gcloud --project arts-612 app deploy app.yaml --no-promote --version ",
    "lint": "eslint \"src/**\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blainegarrett/mplsart-web.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blainegarrett/mplsart-web/issues"
  },
  "homepage": "http://www.mplsart.com/",
  "dependencies": {
    "async": "^2.3.0",
    "better-npm-run": "0.0.8",
    "classnames": "^2.2.5",
    "cross-env": "^2.0.0",
    "emojis-list": "^2.1.0",
    "express": "^4.13.3",
    "flexboxgrid": "^6.3.1",
    "google-maps": "^3.2.1",
    "immutability-helper": "^2.1.2",
    "immutable": "^3.7.6",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "json5": "^0.5.0",
    "moment-timezone": "^0.5.2",
    "object-assign": "^4.1.0",
    "postcss-cssnext": "^2.10.0",
    "postcss-modules-values": "^1.2.2",
    "prop-types": "^15.5.8",
    "react": "^15.1.0",
    "react-addons-css-transition-group": "^15.3.2",
    "react-css-themr": "^1.4.1",
    "react-dom": "^15.1.0",
    "react-flexbox-grid": "^0.10.2",
    "react-helmet": "^2.3.1",
    "react-masonry-component": "^5.0.5",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.0-rc.1",
    "react-toolbox": "^2.0.0-beta.5",
